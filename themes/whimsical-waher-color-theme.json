{
	"$schema": "vscode://schemas/color-theme",
	"name": "Whimsical (Waher)",
	"include": "./whimsical-color-theme.json",
	"colors": {
		"editor.foreground": "#DFC47D",
		"editor.hoverHighlightBackground": "#E0805640",
		"list.highlightForeground": "#E08056"
	},
	"tokenColors": [
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#85C2C2"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#85C2C2"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#DFC47D"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#DFC47D"
			}
		},
		{
			"name": "block scope",
			"scope": [
				"block.scope.end",
				"block.scope.begin",
				"punctuation.definition.block",
				"punctuation.definition.binding-pattern.object"
			],
			"settings": {
				"foreground": "#9F9D6D"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#CEDAE3"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#9F9D6D"
			}
		},
		{
			"name": "braces",
			"scope": [
				"meta.brace.square",
				"meta.brace.round"
			],
			"settings": {
				"foreground": "#9F9D6D"
			}
		},
		{
			"name": "HTML Tags",
			"scope": "entity.name.tag.html",
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"name": "HMTL tag braces",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#DFC47D"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#DFC47D"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#E08056"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#CEDAE3"
			}
		}
	],
	"semanticTokenColors": {
		"newOperator": "#DFC47D",
		"stringLiteral": "#CE9178",
		"customLiteral": "#DFC47D",
		"numberLiteral": "#CEF7EF"
	}
}
